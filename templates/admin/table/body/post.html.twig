{% for post in paginator.results %}
    <tr>
        <td>{{ post.title }}</td>
        <td>{{ post.author.username }}</td>
        <td>{{ post.publishedAt | date('l jS F, Y') }} </td>
        <td>
            <a class="icn__view" href="{{ path('edit_post', {'id':post.id}) }}"></a>
        </td>
        <td>
            <a class="icn__delete" href="{{ path('admin_delete_post', {'id' : post.id}) }}"></a>
        </td>
    </tr>
{% endfor %}